  "scripts": {
    "test:eslint": "npx eslint",
    "test:eslint-fix": "npx eslint --fix --quiet",
    "test:ts": "npx tsc --noEmit --noErrorTruncation --pretty false --incremental false",
    "test:stylelint": "npx stylelint **/*.scss",
    "test:stylelint-fix": "npx stylelint **/*.scss --fix",
    "test:jest": "npx jest",
    "test:jest:watch": "npx jest --watch",
    "test:jest:coverage": "npx jest --coverage",
    "test:screenshots": "npx loki test",
    "test:screenshots-ci": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static && rm -rf storybook-static",
    "test": "npm run test:eslint && npm run test:ts && npm run test:stylelint && npm run test:jest",
    "dev": "cross-env NODE_ENV=development npx webpack --mode development",
    "build": "cross-env NODE_ENV=production npx webpack --mode production",
    "heroku-postbuild": "npm run build",
    "start": "node server.js",
    "start:dev": "cross-env NODE_ENV=development webpack serve --mode development --open",
    "bundle-report": "webpack-bundle-analyzer --port 4200 build/stats.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
